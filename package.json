{
  "name": "stars_of_eternity_bot_saphire_rewrite",
  "version": "1.0.1",
  "description": "",
  "main": "dist/eternity.js",
  "scripts": {
    "start": "node .",
    "dev": "ts-node-dev --compiler ttypescript --project ./src/tsconfig.json --watch ./src ./src/eternity.ts",
    "build": "ttsc -b src",
    "build:start": "npm run build && npm run start",
    "build:clean": "ttsc -b src --clean && npm run build",
    "build:watch": "ttsc -b src -w",
    "typeorm": "ts-node --compiler ttypescript --project ./src/tsconfig.json ./node_modules/typeorm/cli.js",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:generate": "npm run typeorm -- migration:generate -n migration",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "test": "jest ./tests",
    "test:watch": "jest ./tests --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
